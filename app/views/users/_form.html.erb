  <h2><%= t(".title") %></h2>
  <%= form_for @user do |form| %><div class="text-field">
  <%= form.label :email %>
  <%= form.text_field :email, type: 'email' %>
</div>

<div class="password-field">
  <%= form.label :password %>
  <%= form.password_field :password %>
</div>

<div class = "file-field">
  <%= form.file_field :avatars, multiple: true %>	
</div>

<%= form.submit %>
<% end %>



